{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "ignorePackages",
              "jsx": "never",
              "ts": "never",
              "tsx": "never"
            }
          ],
        "no-underscore-dangle": "off",
        "import/prefer-default-export": "off"
    }
}